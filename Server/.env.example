# GitHub Container Registry
GITHUB_REPOSITORY=hidry/brewery

# Application Environment
ASPNETCORE_ENVIRONMENT=Production

# Server Configuration
SERVER_PORT=8800

# Temperature Sensor Addresses (find with: ls /sys/bus/w1/devices/)
TEMP_SENSOR_1=28-xxxxxxxxxxxx
TEMP_SENSOR_2=28-xxxxxxxxxxxx

# GPIO Pin Configuration
GPIO_MIXER=12
GPIO_PIEZO=21

# Logging
LOG_LEVEL=Information
